<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.paulgof.soundwave">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".StartPage"
         android:label="@string/app_name"
         android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".OnlineMod"
            android:label="@string/first_title"
            android:theme="@style/OnlineTheme"/>
        <activity
            android:name=".OfflineMod"
            android:label="@string/second_title"
            android:theme="@style/OfflineTheme"/>
    </application>

</manifest>